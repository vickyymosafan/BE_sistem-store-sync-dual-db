# Environment Configuration
# Copy this file to .env.local for local development

# Node Environment
# Options: development, production, test
NODE_ENV=development

# Server Port (for local development only)
PORT=3000

# Central Database URL (Jember)
# For local development: postgresql://user:password@localhost:5432/indoagustus_central
# For production (Prisma Accelerate): prisma://accelerate.prisma-data.net/?api_key=YOUR_API_KEY
DATABASE_URL_CENTRAL=

# Branch Database URL (Bondowoso)
# For local development: postgresql://user:password@localhost:5433/indoagustus_branch_bondowoso
# For production (Prisma Accelerate): prisma://accelerate.prisma-data.net/?api_key=YOUR_API_KEY
DATABASE_URL_BRANCH_BONDOWOSO=

# Prisma Database URL (used for migrations)
# This should point to one of your databases for running migrations
# Typically set to the same as DATABASE_URL_CENTRAL
DATABASE_URL=
